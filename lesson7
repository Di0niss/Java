    public class Test{static int minNumberPrint(int a, int b){

     if (a>=b){return b;}
    else{return a;}
    }
    public static void main(String[] args){
    System.out.println( minNumberPrint(7,3) );
     }
    }


    public class metod {

        public static void main(String[] args) {

            String str1 = "привет";
            String str2 = "цветок";

